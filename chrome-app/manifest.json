{
  "manifest_version": 2,
  "name": "CloudPaste App",
  "description": "Copy & pastes across all devices on local network",
  "version": "1.0",
	"icons": {
		"16": "icon.png",
		"128": "icon.png"
	},

	"externally_connectable": {
		"ids": ["*"]
	},

	"app": {
		"background": {
			"scripts": ["app.js"]
		}
	},
	"sockets": {
		"udp": {
			"send": ["224.0.0.1:16533"],
			"bind": ["0.0.0.0:16533"]
		},
		"tcpServer": {
			"listen": [":16534"]
		}
	},
  "permissions": [
		"contextMenus", "notifications", "identity", "identity.email"
  ],
	"key": ""
}

