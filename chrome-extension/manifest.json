{
  "manifest_version": 2,
  "name": "LanPaste",
  "description": "Copy & pastes across all devices on local network",
  "version": "1.0",
	"icons": {
		"16": "icon.png",
		"128": "icon.png"
	},
  "background": {
		"scripts": ["background.js"]
	},
  "permissions": [
		"contextMenus", "tabs", "notifications", "activeTab", "identity"
  ],
	"key": ""
}

